<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br>
  <%= f.label :genre %>
  <%= f.select :genre_id, @genres.collect {|g| [g.name, g.id]} %>
  <br>
  <%= f.label :notes %> <br>
  <input name='notes[]' id='song_notes_1'><br>
  <input name='notes[]' id='song_notes_2'><br>

  <%= f.submit %>
<% end %>